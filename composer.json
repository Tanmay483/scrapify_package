{
  "name": "scrapify/pdf-tools",
  "description": "A Laravel package to merge PDF files using Python",
  "type": "library",
  "autoload": {
    "psr-4": {
      "scrapify\\PdfTools\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "scrapify\\PdfTools\\PdfToolsServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.0",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
    "dompdf/dompdf": "^3.1",
    "phpoffice/phpword": "^1.4",
    "phpoffice/phpspreadsheet": "^4.4",
    "smalot/pdfparser": "2.12.0"
  }
}
